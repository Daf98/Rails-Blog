<h1><%= @user.photo %></h1>
<h1><%= @user.name %></h1>
<h2>Number of posts: <%= @user.posts.length %>

<p>Bio: <%= @user.bio %></p>
<h4>Posts</h4>
<% @posts.each_with_index do |post, index| %>
<div>Post #<%= index %>
<p><%= post.title %></p>
<p><%= post.text %></p>
</div>
<% end %>
<a href="<%= @user.id %>/posts">Hello</a>